
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <title>Registration</title>


</head>
<body>
<div id="main" >
  <%= image_tag "logo6.jpg" %>
<%= form_for @user, url: users_path do |f| %>
    <% if @user.errors.any? %>
        <div id="error_explanation">
          <ul>
            <% @user.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <div class="field">
      <%= f.label 'Title' %>
      <%= f.text_field :title, placeholder: "Title" %>
    </div>
    <div class="field">
      <%= f.label 'First name' %>
      <%= f.text_field :fname, placeholder: "First Name" %>
    </div>
    <div class="field">
      <%= f.label 'Last name' %>
      <%= f.text_field :lname, placeholder: "Last Name" %>
    </div>
    <div class="field">
      <%= f.label 'Student Number' %>
      <%= f.text_field :username, placeholder: "Student Number" %>
    </div>
    <div class="field">
      <%= f.label :course_enrolled %>
      <%= f.text_field :course_enrolled, placeholder: 'eg; CK401' %>.
    </div>
    <div class="field">
      <%= f.label :password %>
      <%= f.password_field :password, placeholder: "Password" %>
    </div>
    <div class="field">
      <%= f.label :repeat_password %>
      <%= f.password_field :password_confirmation, placeholder: "Repeat Password" %>
    </div>
    <div class="actions">
      <%= f.submit :create_account, value: "Create Account" %>
    </div>
<% end %>
</div>
</body>

</html>